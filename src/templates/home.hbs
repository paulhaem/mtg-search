<h2>Home</h2>
<form id="solr-query" action=" ">
  <div class="form-group">
    <input id="title-field" class="form-control" name="title" type="text" placeholder="Card Title">
    <input class="btn btn-primary" type="submit" value="Query">
  </div>
  <div id="results">
    {{#each docs}}
      <h2>{{ docs.name }}</h2>
      <div>{{ docs.image }}</div>
      <p>{{docs.text}}</p>
      <h2>Information</h2>
      <table>
        <td>{{docs.cmc}}</td>
        {{#each colors}}
          <td>{{ docs.colors}}</td>
        {{/each}}
        <td>{{docs.power}}</td>
        <td>{{docs.toughness}}</td>
        {{#each subtypes}}
          <td>{{docs.subtypes}}</td>
        {{/each}}
        <td>{{docs.rarity}}</td>
      {{/each}}
    </table>
  </div>
</form>

